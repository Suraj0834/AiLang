{
  "name": "@ailang/react-native",
  "version": "1.0.0",
  "description": "AI-Powered Real-Time Translation Framework for React Native",
  "main": "lib/commonjs/index.js",
  "module": "lib/module/index.js",
  "types": "lib/typescript/index.d.ts",
  "react-native": "src/index.ts",
  "source": "src/index.ts",
  "files": [
    "src",
    "lib",
    "!**/__tests__",
    "!**/__fixtures__",
    "!**/__mocks__",
    "README.md"
  ],
  "scripts": {
    "build": "bob build",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "typescript": "tsc --noEmit",
    "lint": "eslint \"**/*.{js,ts,tsx}\"",
    "lint:fix": "eslint \"**/*.{js,ts,tsx}\" --fix",
    "prepare": "bob build",
    "release": "release-it",
    "prepublishOnly": "npm run build && npm run test"
  },
  "keywords": [
    "react-native",
    "translation",
    "i18n",
    "internationalization",
    "localization",
    "ai",
    "gemini",
    "multilingual"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Suraj0834/AiLang.git"
  },
  "author": "Suraj <suraj6202k@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Suraj0834/AiLang/issues"
  },
  "homepage": "https://ailang.dev",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^1.21.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.11",
    "@types/react": "^18.2.45",
    "@types/react-native": "^0.72.8",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "react": "18.2.0",
    "react-native": "0.73.1",
    "react-native-builder-bob": "^0.23.2",
    "release-it": "^17.0.1",
    "typescript": "^5.3.3"
  },
  "peerDependencies": {
    "react": "*",
    "react-native": "*"
  },
  "jest": {
    "preset": "react-native",
    "modulePathIgnorePatterns": [
      "<rootDir>/lib/"
    ]
  },
  "react-native-builder-bob": {
    "source": "src",
    "output": "lib",
    "targets": [
      "commonjs",
      "module",
      [
        "typescript",
        {
          "project": "tsconfig.build.json"
        }
      ]
    ]
  }
}
